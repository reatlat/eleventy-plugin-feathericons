        .github-fork-ribbon:before { background-color: #333; }
        .prose :where(code):not(:where([class~="not-prose"],[class~="not-prose"] *))::before,
        .prose :where(code):not(:where([class~="not-prose"],[class~="not-prose"] *))::after { content: none !important; }
